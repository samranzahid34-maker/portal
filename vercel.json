{
    "version": 2,
    "builds": [
        {
            "src": "server-simple.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server-simple.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server-simple.js"
        }
    ]
}