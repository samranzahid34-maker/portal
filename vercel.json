{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "main.py"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/public/index-auth.html"
        },
        {
            "src": "/admin.html",
            "dest": "/public/admin.html"
        }
    ]
}